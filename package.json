  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "^12.18.3",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build && gatsby-plugin-s3 deploy -y && aws cloudfront create-invalidation --distribution-id E1XCOP0OJHHXRT --paths \"/*\"",
